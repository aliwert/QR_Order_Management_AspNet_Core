@model UpdateMenuTableDto
@{
    ViewData["Title"] = "UpdateMenuTable";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}


<div class="content">
    <div class="container-fluid">
        <h4 class="page-title">Menu Table Transactions</h4>
       
        <div class="row">
            <div class="col-md-12">
                <form method="post">
                    <div class="card">
                        <div class="card-header">
                            <div class="card-title">Update Table</div>
                        </div>
                        <div class="card-body">
                            <input type="hidden" asp-for="MenuTableID" />
                            <div class="form-group">
                                <label for="name">Enter Table Name</label>
                                <input type="text" class="form-control" asp-for="Name" id="name" placeholder="Table Name">
                            </div>
                        </div>
                        <div class="card-action">
                            <button class="btn btn-primary">Update Table</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>





