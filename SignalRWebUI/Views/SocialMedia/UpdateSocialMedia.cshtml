@model UpdateSocialMediaDto
@{
    ViewData["Title"] = "UpdateSocialMedia";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}
<div class="content">
    <div class="container-fluid">
        <h4 class="page-title">SocialMedia Transactions</h4>
        <div class="row">
            <div class="col-md-12">
                <form method="post">
                    <div class="card">
                        <div class="card-header">
                            <div class="card-title">Update Social Media Account</div>
                        </div>
                        <input type="hidden" asp-for="SocialMediaID" />
                        <div class="card-body">
                            <div class="form-group">
                                <label for="Title">Enter Title</label>
                                <input type="text" class="form-control" asp-for="Title" id="Title" placeholder="Title">
                            </div>
                            <div class="form-group">
                                <label for="Icon">Enter Icon</label>
                                <input type="text" class="form-control" asp-for="Icon" id="Icon" placeholder="Icon">
                            </div>
                            <div class="form-group">
                                <label for="Url">Enter Url</label>
                                <input type="text" class="form-control" asp-for="Url" id="Url" placeholder="Enter Category Name">
                            </div>
                        </div>
                        <div class="card-action">
                            <button class="btn btn-primary">Update Social Media</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>





